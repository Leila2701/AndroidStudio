        android:supportsRtl="true"
        android:theme="@style/Theme.Spinner_personalizado">
        <activity android:name=".ListView_Simple"></activity>
        <activity android:name=".ListView_Personalizado" />
        <activity android:name=".ListView_Formulario" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>